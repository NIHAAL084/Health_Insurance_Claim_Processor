# Debug configuration for Health Insurance Claim Processor
# Override the default settings for maximum debugging

# Logging Configuration
LOG_LEVEL=DEBUG
LOG_FORMAT=detailed
DEBUG=true

# Ollama Configuration
OLLAMA_MODEL=llama3.2:3b
OLLAMA_VERBOSE=true

# FastAPI Configuration
FASTAPI_DEBUG=true
FASTAPI_RELOAD=true

# Google AI Configuration (if using)
GOOGLE_GENAI_USE_VERTEX_AI=false

# Maximum debugging flags
PYTHONPATH=./src
PYTHONVERBOSE=1
PYTHONUNBUFFERED=1

# File processing
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=pdf

# Agent configuration
MAX_PARALLEL_AGENTS=2
AGENT_TIMEOUT=600
